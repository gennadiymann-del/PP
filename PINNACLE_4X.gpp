   endif
   gcode = 0
   {nb, 'G'gcode, ' A'(first_axis_angle)}
   ;{nb, 'G'gcode, ' A'angle}
   if rot_axis_coord ne axis4_cartesian
    ;  {nb, 'M43'}
   endif 
endp

