@change_tool
  
    local logical save_blknum_gen
   skipline =true

;   label = first_user_proc
;   save_blknum_gen = blknum_gen
     {nb, 'M9'}
     {nb, 'G91 G28 Z0 M5'}
     {nb, 'M01'} 
     {nb, 'N'tool_number,' T'tool_number,' M6'}
     {'  ( 'tool_user_type, '  D'(2*tool_offset),'  - ',upper(tool_message)' )'}
     {nb,' T'next_tool_number}

     call @start_tool
         
             call @home_data
             call @home_data1             
             save_part_home_number = part_home_number
     {nb,'G90'}

    gcode = 43
    skipline = FALSE
    ;tool_direction = CCW
    ;call @start_tool
    ;{[' Z'zlnext]}
    blknum_gen = false
    skipline = TRUE

 
    use_m0 = 0
    ch_tool = 1
endp

